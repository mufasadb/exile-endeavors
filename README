# Navigate to the backend directory and build the Docker image
cd path/to/backend
docker build -t exile-endeavors-backend .

# Run the backend container
docker run -d -p 5000:5000 exile-endeavors-backend

# Navigate to the frontend directory and build the Docker image
cd path/to/exile-endeavors-frontend
docker build -t exile-endeavors-frontend .

# Run the frontend container
docker run -d -p 80:80 exile-endeavors-frontend
